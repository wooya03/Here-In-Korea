package kr.kro.hereinkorea.domain.course.dto;

import lombok.Data;

import java.time.LocalDateTime;
import java.util.List;

@Data
public class CourseDTO {
    private Long id; // 코스 아이디
    private String memId; // 회원 아이디
    private String title; // 코스 제목
    private String content; // 코스 내용
    private String tags; // 코스 태그
    private int views; // 조회수
    private int likes; // 좋아요 수
    private LocalDateTime createdDate; // 작성일
    private String courseName; // 코스명
    private List<String> imageUrls; // 코스 이미지 주소 리스트
}
